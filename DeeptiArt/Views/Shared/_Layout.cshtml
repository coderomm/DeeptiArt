<!DOCTYPE html>
<html>
<head>
    <!-- metas -->
    @{
        var title = ViewBag.Title as string;
        if (!string.IsNullOrEmpty(title))
        {
            var textInfo = System.Globalization.CultureInfo.CurrentCulture.TextInfo;
            title = textInfo.ToTitleCase(title);
        }
    }
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@title | Deepti Art</title>
    <meta name="description" content="Deepti Art offers a collection of pencil sketches and drawings created by Ms. Deepti Heda, an artist based in Mumbai, Maharashtra. Explore her beautiful artwork.">
    <meta name="keywords" content="Deepti Art, pencil sketches, drawings, artist, Mumbai, Maharashtra, Bhilwara, Rajasthan">
    <meta name="author" content="Ms. Deepti Heda">

    <!-- Define canonical URL for SEO -->
    <link rel="canonical" href="https://deeptiart.com/">
    <link rel="shortcut icon" href="~/Content/assets/img/favicon.ico">

    <!-- Add Open Graph meta tags for social media sharing -->
    <meta property="og:title" content="Deepti Art - Pencil Sketches and Drawings">
    <meta property="og:description" content="Deepti Art offers a collection of pencil sketches and drawings created by Ms. Deepti Heda, an artist based in Mumbai, Maharashtra. Explore her beautiful artwork.">
    <meta property="og:image" content="https://deeptiart.com/images/cover.jpg">
    <meta property="og:url" content="https://deeptiart.com/">
    <meta property="og:type" content="website">

    <!-- CSS Template -->
    <link href="~/Content/assets/css/style.css" rel="stylesheet" />

    <!-- JQuery / Ajax -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>

    <!-- Fontawesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css">

    <!--Google Fonts-->

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Cinzel+Decorative&family=Montserrat&family=Proza+Libre&display=swap" rel="stylesheet">
    <!-- JSON-LD Structured Data -->
    <script type="application/ld+json">
        {
            "@@context": "https://schema.org",
            "@@type": "CreativeWork",
            "name": "Pencil Sketch: Sunset",
            "author": {
                "@@type": "Person",
                "name": "Ms. Deepti Heda"
            },
            "dateCreated": "2023-09-22",
            "description": "A beautiful pencil sketch of a sunset over the ocean."
        }
    </script>
