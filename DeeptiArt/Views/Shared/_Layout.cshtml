    <div class="modal-size-chart modal fade" id="px_size_chart_modal" tabindex="-1">
        <div class="modal-dialog modal-dialog-centered modal-xl">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Size Chart</h4>
                    <button class="btn-close" type="button" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <table class="table mb-0">
                        <thead>
                            <tr class="text-mode">
                                <th>Size in Inches</th>
                                <th>Size in Centimeters (cm)</th>
                                <th>Size in Millimeters (mm)</th>
                                <th>Size in Pixels (at 96 DPI)</th>
                                <th>Size in Points (pt)</th>
                                <th>Size in Picas (pc)</th>
                            </tr>
                        </thead>
                        <tbody>
                            @for (int inch = 5; inch <= 16; inch++)
                            {
                                double cm = inch * 2.54;
                                double mm = cm * 10;
                                double pixels = inch * 96; // Assuming 96 DPI
                                double points = inch * 72;
                                double picas = inch * 6;

                                <tr>
                                    <td>@(inch).00"</td>
                                    <td>@cm.ToString("0.00") cm</td>
                                    <td>@mm.ToString("0.00") mm</td>
                                    <td>@pixels.ToString("0.00") px</td>
                                    <td>@points.ToString("0.00") pt</td>
                                    <td>@picas.ToString("0.00") pc</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <!-- End Size Chart  -->
